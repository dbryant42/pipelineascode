pipeline {
    agent any
    stages {
        stage('First') {
                steps {
                        echo 'Hi, I am Dwalu this is my first pipeline class'
			
                }
        }
         stage('Second') {
                steps {
                        echo 'I live in USA'
			
                }
        }
	stage('Third') {
		steps {
			echo 'Hi again, This is Dwalu your pipeline master in marland'
		}
		}
	stage('Forth') {
		steps {
			echo 'Good evening, I am your Guy, the Jenkins master in Maryland'
		}
	}
      }
 }
